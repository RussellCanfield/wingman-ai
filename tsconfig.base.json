{
	"compilerOptions": {
		"target": "ES2021",
		"lib": ["ES2021"],
		"module": "commonjs",
		"outDir": "out",
		"sourceMap": true,
		"rootDir": "extension/src",
		"composite": true,
		"strict": true,
		"skipLibCheck": true,
		"esModuleInterop": true
	},
	"exclude": [
		"**/node_modules",
		".vscode-test",
		"apps/docs-website",
		"docs",
		"extension/scripts",
		"media",
		"views-ui",
		"audio",
		".github"
	]
}
