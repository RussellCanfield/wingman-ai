{
	"name": "wing-man",
	"displayName": "Wingman-AI",
	"description": "Wingman - AI powered assistant to help you write your best code, we won't leave you hanging.",
	"version": "0.7.5",
	"publisher": "WingMan",
	"license": "MIT",
	"workspaces": [
		"docs-site",
		"shared",
		"views-ui"
	],
	"authors": [
		[
			{
				"name": "Russell Canfield",
				"email": "rcanfield86@gmail.com"
			},
			{
				"name": "Harlen Alvarez",
				"email": "harlenalvarez@gmail.com"
			}
		]
	],
	"icon": "media/icon.png",
	"galleryBanner": {
		"color": "#262626",
		"theme": "dark"
	},
	"contributors": [
		{
			"name": "Russell Canfield",
			"email": "rcanfield86@gmail.com"
		},
		{
			"name": "Harlen Alvarez",
			"email": "harlenalvarez@gmail.com"
		}
	],
	"repository": {
		"type": "git",
		"url": "https://github.com/RussellCanfield/wingman-ai"
	},
	"engines": {
		"vscode": "^1.93.0"
	},
	"categories": [
		"Programming Languages",
		"Snippets",
		"Machine Learning",
		"Education",
		"AI",
		"Chat"
	],
	"pricing": "Free",
	"keywords": [
		"ai",
		"codestral",
		"chatgpt",
		"copilot",
		"compose",
		"sonnet",
		"intellisense",
		"openai",
		"anthropic",
		"composer",
		"gpt",
		"chat",
		"ollama",
		"huggingface",
		"code assistant"
	],
	"activationEvents": [
		"onStartupFinished",
		"onLanguage:plaintext"
	],
	"main": "./out/extension.js",
	"contributes": {
		"icons": {
			"wingman-logo": {
				"description": "Wingman icon",
				"default": {
					"fontPath": "media/wingman.woff",
					"fontCharacter": "\\e900"
				}
			}
		},
		"languages": [
			{
				"id": "plaintext",
				"aliases": [
					"Plain Text",
					"plaintext"
				],
				"extensions": [
					".txt",
					".text"
				]
			}
		],
		"viewsContainers": {
			"activitybar": [
				{
					"id": "wing-man",
					"title": "Wingman",
					"icon": "media/wingman-logo.png"
				}
			],
			"panel": [
				{
					"id": "wing-man-config",
					"title": "Wingman Config",
					"icon": "media/wingman-logo.png"
				}
			]
		},
		"views": {
			"wing-man": [
				{
					"id": "wingman.chatview",
					"name": "We've got your back!",
					"type": "webview"
				}
			],
			"wing-man-config": [
				{
					"id": "wingman.configview",
					"name": "Wingman Config",
					"type": "webview"
				}
			]
		},
		"commands": [
			{
				"command": "wingmanai.gendocument",
				"title": "Wingman: Gen Docs"
			},
			{
				"command": "wingmanai.refactorcode",
				"title": "Wingman: Refactor Code"
			},
			{
				"command": "wingmanai.triggercodecomplete",
				"title": "Wingman: Code Complete"
			},
			{
				"command": "wingmanai.opencomposer",
				"title": "Wingman: Composer"
			}
		],
		"keybindings": [
			{
				"command": "wingmanai.triggercodecomplete",
				"key": "ctrl+shift+space"
			},
			{
				"command": "wingmanai.opencomposer",
				"key": "ctrl+i",
				"mac": "cmd+i"
			}
		]
	},
	"scripts": {
		"start:views": "cd views-ui && npm run dev",
		"build": "rsbuild build",
		"build:views": "cd views-ui && npm run build",
		"build:shared": "cd shared && npm run build",
		"build:docs": "cd docs-site && npm run build",
		"vscode:prepublish": "npm run compile",
		"compile": "npm run clean && node ./scripts/build.js && npm run build && npm run build:shared && npm run build:views",
		"watch": "tsc -b --watch",
		"watch:views": "npm run clean && NODE_ENV=development npm run build:views && NODE_ENV=development npm run build",
		"pretest": "npm run compile && npm run lint",
		"lint": "eslint src --ext ts",
		"clean": "rimraf out",
		"docs": "cd docs-site && npm run dev"
	},
	"dependencies": {
		"@ast-grep/napi": "0.27.1",
		"@langchain/anthropic": "0.3.1",
		"@langchain/community": "0.3.1",
		"@langchain/core": "0.3.3",
		"@langchain/langgraph": "0.2.8",
		"@langchain/ollama": "0.1.0",
		"@langchain/openai": "0.3.0",
		"ignore": "5.2.0",
		"langchain": "0.3.2",
		"node-cache": "^5.1.2",
		"tree-sitter": "0.21.1",
		"vectra": "0.9.0",
		"vscode-languageclient": "9.0.1",
		"vscode-languageserver": "9.0.1",
		"vscode-languageserver-textdocument": "1.0.12",
		"vscode-uri": "3.0.8",
		"zod": "3.23.8",
		"zod-to-json-schema": "3.23.3"
	},
	"devDependencies": {
		"@ast-grep/cli": "0.27.1",
		"@rsbuild/core": "1.0.6",
		"@rsbuild/plugin-react": "1.0.2",
		"@types/mocha": "10.0.8",
		"@types/node": "22.5.5",
		"@types/vscode": "1.93.0",
		"@types/vscode-webview": "1.57.5",
		"@vscode/test-cli": "0.0.10",
		"@vscode/test-electron": "2.4.1",
		"adm-zip": "0.5.16",
		"generate-license-file": "3.5.1",
		"node-fetch": "3.3.2",
		"rimraf": "^6.0.1",
		"tar": "7.4.3",
		"typescript": "^5.6.2"
	}
}
