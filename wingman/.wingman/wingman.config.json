{
	"logLevel": "info",
	"recursionLimit": 5000,
	"defaultAgent": "main",
	"gateway": {
		"host": "0.0.0.0",
		"port": 18789,
		"auth": { "mode": "token" },
		"fsRoots": [
			"~/.wingman/outputs",
			"~/Projects"
		],
		"controlUi": {
			"enabled": true,
			"port": 18789
		}
	},
	"agents": {
		"list": [
			{ "id": "main", "name": "Main", "default": true },
			{ "id": "coding", "name": "Coding" },
			{ "id": "researcher", "name": "Researcher" },
			{ "id": "stock-trader", "name": "Stock Trader" }
		],
		"bindings": [
			{ "agentId": "main", "match": { "channel": "webui" } }
		]
	},
	"mcp": {
		"servers": [
			{
				"name": "finnhub",
				"transport": "stdio",
				"command": "bun",
				"args": ["./src/tools/mcp-finance.ts"],
				"env": {
					"FINNHUB_API_KEY": "${FINNHUB_API_KEY}"
				}
			}
		]
	}
}
