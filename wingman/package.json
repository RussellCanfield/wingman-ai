{
	"name": "@wingman-ai/gateway",
	"displayName": "Wingman Agent",
	"description": "Wingman-AI agentic coding assistant that works with Anthropic, OpenAI, Google, xAI and LMStudio.",
	"version": "1.1.5",
	"main": "index.js",
	"bin": {
		"wingman": "./bin/wingman"
	},
	"private": false,
	"type": "module",
	"license": "MIT",
	"files": [
		"dist",
		"bin",
		".wingman/agents"
	],
	"publishConfig": {
		"access": "public"
	},
	"packageManager": "bun@1.1.42",
	"scripts": {
		"test": "vitest",
		"test:unit": "vitest run --dir src/tests --exclude '**/*integration*'",
		"test:integration": "vitest run --dir src/tests/integration",
		"test:coverage": "vitest run --coverage",
		"test:watch": "vitest watch",
		"test:watch:unit": "vitest watch --dir src/tests --exclude '**/*integration*'",
		"test:watch:integration": "vitest watch --dir src/tests/integration",
		"build": "rslib build && bun run webui:build",
		"dev": "concurrently -n gateway,webui -c emerald,orange \"bun --watch ./src/cli/index.ts gateway run\" \"bun run webui:dev\"",
		"dev:gateway": "bun --watch ./src/cli/index.ts gateway run",
		"dev:webui": "bun run webui:dev",
		"int": "bun ./examples/agent-test.ts",
		"cli": "./bin/wingman",
		"cli:help": "./bin/wingman --help",
		"cli:list": "./bin/wingman agent",
		"webui:dev": "vite --config webui/vite.config.ts --host",
		"webui:build": "vite build --config webui/vite.config.ts",
		"webui:preview": "vite preview --config webui/vite.config.ts",
		"gateway:example": "bun ./examples/gateway-example.ts",
		"gateway:start": "./bin/wingman gateway start",
		"gateway:stop": "./bin/wingman gateway stop",
		"gateway:status": "./bin/wingman gateway status",
		"mcp:finance": "bun ./src/tools/mcp-finance.ts"
	},
	"exports": {
		".": {
			"types": "./dist/index.d.ts",
			"import": "./dist/index.js",
			"require": "./dist/index.js"
		},
		"./gateway": {
			"types": "./dist/gateway/index.d.ts",
			"import": "./dist/gateway/index.js",
			"require": "./dist/gateway/index.js"
		}
	},
	"authors": [
		[
			{
				"name": "Russell Canfield",
				"email": "rcanfield86@gmail.com"
			}
		]
	],
	"contributors": [
		{
			"name": "Russell Canfield",
			"email": "rcanfield86@gmail.com"
		}
	],
	"repository": {
		"type": "git",
		"url": "https://github.com/RussellCanfield/wingman-ai"
	},
	"overrides": {
		"zod": "^4.3.5"
	},
	"dependencies": {
		"@langchain/anthropic": "^1.3.10",
		"@langchain/community": "^1.1.4",
		"@langchain/core": "^1.1.15",
		"@langchain/google-genai": "^2.1.10",
		"@langchain/langgraph": "1.1.0",
		"@langchain/langgraph-checkpoint-sqlite": "^1.0.0",
		"@langchain/mcp-adapters": "^1.1.1",
		"@langchain/openai": "^1.2.2",
		"@langchain/xai": "^1.2.1",
		"@modelcontextprotocol/sdk": "^1.20.2",
		"@clack/prompts": "^0.7.0",
		"chalk": "^5.6.2",
		"deepagents": "^1.5.0",
		"duck-duck-scrape": "^2.2.7",
		"ink": "^5.2.1",
		"ink-box": "^2.0.0",
		"ink-spinner": "^5.0.0",
		"js-yaml": "^4.1.1",
		"langchain": "^1.2.10",
		"multicast-dns": "^7.2.5",
		"react": "^18.3.1",
		"react-dom": "^18.3.1",
		"react-icons": "^5.3.0",
		"react-markdown": "^9.0.1",
		"reactflow": "^11.11.4",
		"react-router-dom": "^6.26.2",
		"remark-gfm": "^4.0.0",
		"uuid": "^13.0.0",
		"zod": "^4.3.5"
	},
	"devDependencies": {
		"@biomejs/biome": "^2.3.13",
		"@rslib/core": "0.19.2",
		"@types/bun": "1.3.6",
		"@types/js-yaml": "^4.0.9",
		"@types/multicast-dns": "^7.2.4",
		"@types/react": "^18.3.3",
		"@types/react-dom": "^18.3.0",
		"@types/uuid": "^10.0.0",
		"@vitejs/plugin-react": "^4.3.4",
		"autoprefixer": "^10.4.20",
		"concurrently": "^8.2.2",
		"postcss": "^8.4.49",
		"tailwindcss": "^3.4.16",
		"typescript": "^5.9.3",
		"vite": "^5.4.11",
		"vitest": "^4.0.17"
	}
}
